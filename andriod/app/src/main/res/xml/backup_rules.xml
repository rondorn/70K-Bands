<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include the entire 70kBands directory first, then exclude specific files -->
    <include domain="file" path="70kBands/" />
    
    <!-- Exclude cache files and temporary data -->
    <exclude domain="file" path="70kBands/bandListCache.data" />
    <exclude domain="file" path="70kBands/backupFileTemp.zip" />
    <exclude domain="file" path="70kBands/cachedImages/" />
    <exclude domain="file" path="70kBands/.nomedia" />
</full-backup-content>
